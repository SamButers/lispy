?start : expr

?expr : atom | list

?number : INT | FLOAT

?atom : (number | STRING | SYMBOL) -> atom

?list : "(" expr* ")" -> list

// Terminais
SYMBOL.0  : /[-!+\/*@$%^#&~<>?|\\\w=]+/
STRING    : /"[^"\\]*(\\[^\n\t\r\f][^"\\]*)*"/
INT.1     : /-?\d+/
FLOAT.2   : /-?\d+\.\d+/

%ignore /\s+/
%ignore /;[^\n]*/